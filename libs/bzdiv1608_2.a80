;2.2 Classic 16-bit / 8-bit Unsigned
;Input: HL = Dividend, C = Divisor, A = 0
;Output: HL = Quotient, A = Remainder (see note)
bzdiv1608:
        xor     a
	REPT    16
	 add	hl,hl		; unroll 16 times
	 rla			; ...
	 cp	c		; ...
	 jr	c,$+4		; ...
	 sub	c		; ...
	 inc	l		; ...
	ENDM
	ret
